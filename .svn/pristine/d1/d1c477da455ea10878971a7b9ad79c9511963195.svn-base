package ino.web.freeBoard.dto;

import org.apache.ibatis.type.Alias;

@Alias("refBoardDto")
public class RefBoardDto {

	private int refnum;
	private String reftitle;
	private String refname;
	private String refregdate;
	private String refcontent;
	private int refreadcnt;
	
	// 페이징변수
	private int startIndex;
	private int cntPerPage;
	

	public int getRefnum() {
		return refnum;
	}


	public void setRefnum(int refnum) {
		this.refnum = refnum;
	}


	public String getReftitle() {
		return reftitle;
	}


	public void setReftitle(String reftitle) {
		this.reftitle = reftitle;
	}


	public String getRefname() {
		return refname;
	}


	public void setRefname(String refname) {
		this.refname = refname;
	}


	public String getRefregdate() {
		return refregdate;
	}


	public void setRefregdate(String refregdate) {
		this.refregdate = refregdate;
	}


	public String getRefcontent() {
		return refcontent;
	}


	public void setRefcontent(String refcontent) {
		this.refcontent = refcontent;
	}


	public int getRefreadcnt() {
		return refreadcnt;
	}


	public void setRefreadcnt(int refreadcnt) {
		this.refreadcnt = refreadcnt;
	}


	public int getRefStartIndex() {
		return startIndex;
	}


	public void setRefStartIndex(int startIndex) {
		this.startIndex = startIndex;
	}


	public int getRefCntPerPage() {
		return cntPerPage;
	}


	public void setRefCntPerPage(int cntPerPage) {
		this.cntPerPage = cntPerPage;
	}


	@Override
	public String toString() {
		return "RefBoardDto [refnum=" + refnum + ", reftitle=" + reftitle + ", refname=" + refname + ", refregdate=" + refregdate
				+ ", refcontent=" + refcontent + ", refreadcnt=" + refreadcnt + "]";
	}
	
}
